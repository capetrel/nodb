MAIL_ENCRYPTION=null
MAIL_SENDER_ADDRESS=no-reply@demo.fr
MAIL_SENDER_NAME="${APP_NAME}"
MAIL_ADMIN_ADDRESS=admin@demo.fr
MAIL_ADMIN_NAME="admin"