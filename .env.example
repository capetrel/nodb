# this var is usefull to generate assets URL in the view_helper.php
# set this var to /public if vhost can't be modified and if /public folder must be rename set the new name
DOC_ROOT=/

APP_NAME=example
APP_ENV=local
APP_DEBUG=true
APP_URL=example.test
APP_META_DESC="Default content in meta description"

MAIL_DRIVER=smtp
MAIL_HOST=your_smtp_host
